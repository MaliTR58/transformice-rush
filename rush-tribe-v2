tfm.exec.newGame(maps[math.random(#maps)])

gamesettings={}
jump={}
teams={}
team=0
recess=0
gamekey=0 ---GameKey = Game is = 0(on) 1 = (off)

pointred=0
pointyellow=0

--- Q and E Skils distance
local ara = 50
--- Q and E Skils distance

function eventNewPlayer(name)

--- Help Language




--- Help ---
ui.addTextArea(81, "<p align='center'><a href='event:close'>CLOSE", name, 373.5, 337, 50, 20,0x000000,0x000001,500,true)
ui.addTextArea(82, "<p align='center'><a href='event:close'><br><b><font size='15' color='#FD0065'>Abilities:</font></b><br><br><b><font size='15' color='#DBDBDB'>Q</font></b> Takes 1 of 2 health of nearby Mouse</b><br><br><b><font size='15' color='#DBDBDB'>E</font>:</b> Freezes nearby Mouse briefly.<br><br><b><font size='15' color='#DBDBDB'>F</font>:</b> As long as you hold it down it hides you like a ninja..<br><br><font size='15' color='#DBDBDB'>Down Key:</font> The longer you press, the higher it jumps.<br><br> Go to the Birthplace of the opposing team and earn 1 point for your team. The team that reaches 3 points wins!<br><br>Coder: Muco#2311 <br>Host: -", name, 200, 76, 400, 250,0x000000,0x000001,500,true)

--- Help ---

--- Key Permisson Start 
tfm.exec.bindKeyboard(name, 40, true, true)
tfm.exec.bindKeyboard(name, 40, false, true)
tfm.exec.bindKeyboard(name, 83, true, true)
tfm.exec.bindKeyboard(name, 83, false, true)
tfm.exec.bindKeyboard(name, 81, true, true)
tfm.exec.bindKeyboard(name, 69, true, true)
tfm.exec.bindKeyboard(name, 70, true, true)
tfm.exec.bindKeyboard(name, 70, false, true)
--- Key Permisson Finsh






--- Algorithm Start
teams[name]={team=0}
gamesettings[name]={q1=0,q2=0,e1=0,e2=0,efreezeend=0,f1=0,f2=0,fx=0,fy=0,qtime1=0,qtime2=0,etime1=0,etime2=0,ftime1=0,ftime2=0,health=2} 
jump[name]={jump1=0,jump2=0}

--- Algorithm Finsh

----mapname
tfm.exec.setUIMapName("<J>R<t>u<ch>s<rose>h </rose> <j>"..pointyellow.."</j>-<rose>"..pointred.."",nil,53,30,700,nil,0,1,0,true)
----mapname


--- Q E F Skils and Health
ui.addTextArea(0,"<font size='25' color='#DBDBDB'> S",name,170,370,40,35,0x000000,0x000001,500,true)
ui.addTextArea(1,"<font size='25' color='#DBDBDB'> Q",name,10,360,40,35,0x000000,0x000001,500,true)
ui.addTextArea(2,"<font size='25' color='#DBDBDB'> E",name,60,370,40,30,0x000000,0x000001,500,true)
ui.addTextArea(3,"<font size='25' color='#DBDBDB'> F",name,110,370,50,30,0x000000,0x000001,500,true)

ui.addTextArea(10,"<font size='10' color='#9D2A2A'> Jump",name,170,340,50,35,0x000000,0x000000,500,true)
ui.addTextArea(11,"<font size='10' color='#9D2A2A'> Damage",name,0,330,75,20,0x000000,0x000000,500,true)
ui.addTextArea(21,"<font size='10' color='#9D2A2A'> Freeze",name,60,340,50,20,0x000000,0x000000,500,true)
ui.addTextArea(31,"<font size='10' color='#9D2A2A'> Ninja",name,120,340,50,20,0x000000,0x000000,500,true)

ui.addTextArea(4,"<font size='15' color='#DBDBDB'> Health ❤: <font size='20' color='#EF5A5A'>"..gamesettings[name].health,name,10,20,120,25,0x000000,0x000001,500,true)
--- Q E F Skils Health


--- Respawn
tfm.exec.respawnPlayer(name)
--- Respawn

tfm.exec.setUIMapName("<J>R<t>u<ch>s<rose>h </rose> <j>"..pointyellow.."</j>-<rose>"..pointred.."",nil,53,30,700,nil,0,1,0,true)

--- Team Red or Yellow Start
print(gamekey)
if gamekey==0 then
if team==0 then
tfm.exec.movePlayer(name,2970,770)
tfm.exec.setNameColor(name,0xF077AF)
teams[name].team=1
team=team+1
elseif team==1 then
tfm.exec.movePlayer(name,20,770)
tfm.exec.setNameColor(name,0xCDCE20)
teams[name].team=2
team=team-1
end
end
--- Team Red or Yellow Finsh
end



function eventKeyboard(name, key, down, x, y)

if gamekey==0 then

---Jump Start

if key == 40  or key==83 then
if down then
jump[name].jump1=1
else
tfm.exec.movePlayer(name,0,0,true,0,jump[name].jump2,false)
jump[name].jump1=0
jump[name].jump2=0
ui.addTextArea(0,"<font size='25' color='#DBDBDB'> S",name,170,370,40,35,0x000000,0x000001,500,true)
end
end
---Jump End

--- F no skils
if tfm.get.room.playerList[name].y>0 then
--- Skill Q Start
if key == 81 then
if gamesettings[name].qtime1==0 then
gamesettings[name].qtime1=1
gamesettings[name].qtime2=15
if gamesettings[name].q1==0 then
for o, data in pairs(tfm.get.room.playerList) do
local xU, yU = math.abs(x - data.x), math.abs(y - data.y)
if xU < ara and yU < ara and o ~= name then
if teams[name].team==1 then
if teams[o].team==2 then
if gamesettings[o].health==2 then
gamesettings[o].health=gamesettings[o].health-1
else
tfm.exec.killPlayer(o)
gamesettings[o].health=2
tfm.exec.respawnPlayer(o)
tfm.exec.displayParticle(5,tfm.get.room.playerList[o].x,tfm.get.room.playerList[o].y,0,0,0,0,nil)
ui.addTextArea(4,"<font size='15' color='#DBDBDB'> Health: <font size='20' color='#EF5A5A'>"..gamesettings[o].health,o,10,20,120,25,0x000000,0x000001,500,true)
end
end
end
if teams[name].team==2 then
if teams[o].team==1 then
if gamesettings[o].health==2 then
gamesettings[o].health=gamesettings[o].health-1
else
tfm.exec.killPlayer(o)
gamesettings[o].health=2
tfm.exec.respawnPlayer(o)
tfm.exec.displayParticle(5,tfm.get.room.playerList[o].x,tfm.get.room.playerList[o].y,0,0,0,0,nil)
ui.addTextArea(4,"<font size='15' color='#DBDBDB'> Health: <font size='20' color='#EF5A5A'>"..gamesettings[o].health,o,10,20,120,25,0x000000,0x000001,500,true)
end
end
end
end
end
end
end
end
--- Skill E End

if key == 69 then

if gamesettings[name].etime1==0 then
gamesettings[name].etime1=1
gamesettings[name].etime2=12
if gamesettings[name].e1==0 then
for o, data in pairs(tfm.get.room.playerList) do
local xU, yU = math.abs(x - data.x), math.abs(y - data.y)
if xU < ara and yU < ara and o ~= name then
if teams[name].team==1 then
if teams[o].team==2 then
tfm.exec.displayParticle(6,tfm.get.room.playerList[o].x,tfm.get.room.playerList[o].y,0,0,0,0,nil)
tfm.exec.freezePlayer(o,true)
gamesettings[o].e2=1
gamesettings[o].efreezeend=4
end
end
if teams[name].team==2 then
if teams[o].team==1 then
tfm.exec.displayParticle(6,tfm.get.room.playerList[o].x,tfm.get.room.playerList[o].y,0,0,0,0,nil)
tfm.exec.freezePlayer(o,true)
gamesettings[o].e2=1
gamesettings[o].efreezeend=4
end
end
end
end
end
end
end
--- Skill E End



end
--- F no skils
--- Skill F Start



if key == 70 then 
if gamesettings[name].ftime1==0 then
if down then
gamesettings[name].fx=tfm.get.room.playerList[name].x
gamesettings[name].fy=tfm.get.room.playerList[name].y
tfm.exec.movePlayer(name,1500,-100,false)
else

tfm.exec.movePlayer(name,gamesettings[name].fx,gamesettings[name].fy,false)
gamesettings[name].ftime1=1
gamesettings[name].ftime2=30
end
end
end

--- Skill F End





end
end

----- Start Loop


function eventLoop()

-----for start -----
if gamekey==0 then
for name,i in pairs(tfm.get.room.playerList) do

------- Yellow Win -------

if tfm.get.room.playerList[name].x>2800 and tfm.get.room.playerList[name].x<3000 and tfm.get.room.playerList[name].y>650 and tfm.get.room.playerList[name].y<800 then
if teams[name].team==2 then
tfm.exec.killPlayer(name)
pointyellow=pointyellow+1
tfm.exec.setUIMapName("<J>R<t>u<ch>s<rose>h </rose> <j>"..pointyellow.."</j>-<rose>"..pointred.."",nil,53,30,700,nil,0,1,0,true)
end
end




------- Yellow Win -------

------- Red Win -------




if tfm.get.room.playerList[name].x>0 and tfm.get.room.playerList[name].x<200 and tfm.get.room.playerList[name].y>650 and tfm.get.room.playerList[name].y<800 then
if teams[name].team==1 then
tfm.exec.killPlayer(name)
pointred=pointred+1
tfm.exec.setUIMapName("<J>R<t>u<ch>s<rose>h </rose> <j>"..pointyellow.."</j>-<rose>"..pointred.."",nil,53,30,700,nil,0,1,0,true)
end
end

------- Red Win -------
end
end
-----for end -----


-- game recess
if recess>0 then
recess=recess-0.5
tfm.exec.setUIMapName("<J>"..recess.."</J>",nil,53,30,700,nil,0,1,0,true)
end


if recess==1 then
tfm.exec.newGame(maps[math.random(#maps)])
gamekey=0
end

if recess==0 then
recess=-1
gamekey=0
for name,i in pairs(tfm.get.room.playerList) do
if teams[name].team==1 then
tfm.exec.movePlayer(name,2970,770)
tfm.exec.setNameColor(name,0xF077AF)
elseif teams[name].team==2 then
tfm.exec.movePlayer(name,20,770)
tfm.exec.setNameColor(name,0xCDCE20)
end
end
tfm.exec.setUIMapName("<J>R<t>u<ch>s<rose>h </rose> <j>"..pointyellow.."</j>-<rose>"..pointred.."",nil,53,30,700,nil,0,1,0,true)
end
-- game recess




---- Win Yellow -----
if pointyellow>=3 then
gamekey=1
pointred=0
pointyellow=0
recess=10
tfm.exec.newGame([[<C><P F="0" /><Z><S><S L="800" H="80" X="400" Y="360" T="17" P="0,0,0.3,0.2,0,0,0,0" /></S><D><DS Y="305" X="398" /><T Y="321" X="400" /></D><O /></Z></C>]])
end

---- Win Yellow -----




---- Win Red -----
if pointred>=3 then
gamekey=1
pointred=0
pointyellow=0
recess=10
tfm.exec.newGame([[<C><P F="0" /><Z><S><S L="800" H="80" X="400" Y="360" T="18" P="0,0,0.3,0.2,0,0,0,0" /></S><D><DS Y="305" X="398" /><T Y="321" X="400" /></D><O /></Z></C>]])
end

---- Win Red -----




----Q E F skils Cooldown Start

---- Q -----
for name,player in pairs(tfm.get.room.playerList) do

if gamesettings[name].qtime1==1 then
if gamesettings[name].qtime2==0 then
ui.addTextArea(1,"<font size='25' color='#DBDBDB'> Q",name,10,360,40,35,0x000000,0x000001,500,true)
gamesettings[name].qtime1=0
else
gamesettings[name].qtime2=gamesettings[name].qtime2-1
ui.addTextArea(1,"<font size='20' color='#DBDBDB'> "..gamesettings[name].qtime2,name,10,360,40,35,0x000000,0x000001,500,true)
end
end
---- Q -----

---- E -----
if gamesettings[name].etime1==1 then
if gamesettings[name].etime2==0 then
ui.addTextArea(2,"<font size='25' color='#DBDBDB'> E",name,60,370,40,30,0x000000,0x000001,500,true)
gamesettings[name].etime1=0
else
gamesettings[name].etime2=gamesettings[name].etime2-1
ui.addTextArea(2,"<font size='20' color='#DBDBDB'> "..gamesettings[name].etime2,name,60,370,40,30,0x000000,0x000001,500,true)
end
end
---- E -----

----- E Freeze end
if gamesettings[name].e2==1 then
if gamesettings[name].efreezeend>1 then
gamesettings[name].efreezeend=gamesettings[name].efreezeend-0.5
end

if gamesettings[name].efreezeend==1 then

gamesettings[name].efreezeend=0
tfm.exec.freezePlayer(name,false)
end

end

----- E Freeze end

---- F -----
if gamesettings[name].ftime1==1 then
if gamesettings[name].ftime2==0 then
ui.addTextArea(3,"<font size='25' color='#DBDBDB'> F",name,110,370,50,30,0x000000,0x000001,500,true)
gamesettings[name].ftime1=0
else
gamesettings[name].ftime2=gamesettings[name].ftime2-1
ui.addTextArea(3,"<font size='20' color='#DBDBDB'> "..gamesettings[name].ftime2,name,110,370,50,30,0x000000,0x000001,500,true)
end
end

---- F -----


end
----Q E F skils cooldown end






---Jump Loop Start
for name,player in pairs(tfm.get.room.playerList) do
if jump[name].jump1==1 then
if jump[name].jump1>-119 then
jump[name].jump2=jump[name].jump2-30
end
end

if jump[name].jump2==-30 then
ui.addTextArea(0,"<font size='25' color='#FFD0D0'> S",name,170,370,40,35,0x000000,0x000001,500,true)
end
if jump[name].jump2==-60 then
ui.addTextArea(0,"<font size='25' color='#FF9191'> S",name,170,370,40,35,0x000000,0x000001,500,true)
end
if jump[name].jump2==-90 then
ui.addTextArea(0,"<font size='25' color='#D44F4F'> S",name,170,370,40,35,0x000000,0x000001,500,true)
end
if jump[name].jump2==-120 then
ui.addTextArea(0,"<font size='25' color='#C90D0D'> S",name,170,370,40,35,0x000000,0x000001,500,true)
end
end
---Jump Loop End






end




function eventPlayerDied(name)
gamesettings[name].fx=0
gamesettings[name].fy=0
--- Respawn
if gamekey==0 then
tfm.exec.respawnPlayer(name)


if teams[name].team==1 then

tfm.exec.movePlayer(name,2970,770)
tfm.exec.setNameColor(name,0xF077AF)
end
if teams[name].team==2 then
tfm.exec.movePlayer(name,20,770)
tfm.exec.setNameColor(name,0xCDCE20)
end

end
--- Respawn
end
function eventTextAreaCallback(id, name, callback)
if callback=="close" then
ui.removeTextArea(81,name)
ui.removeTextArea(82,name)
end
end


for name,player in pairs(tfm.get.room.playerList) do
eventNewPlayer(name)
end
